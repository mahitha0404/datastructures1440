#include<stdio.h>
int fibbonacci(int n)
{
	if(n==0){
	return 0;}
	
else if(n==1)
{
	return 1;
}else 
{
	return (fibbonacci(n-1)+fibbonacci(n-2));
}
}
int main()
{
	int n;
	int i;
	printf("enter the number:");
	scanf("%d",&n);
	printf("fibbonacci of %d :",&n);
	
	for(i=0;i<n;i++)
	{
		printf("%d",fibbonacci(i));
	}
}


output:
Enter the number of elements:4

0 1 1 2

